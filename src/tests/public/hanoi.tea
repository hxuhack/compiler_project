fn move(x:int, y:int)
{
    putint(x); putch(32); putint(y); putch(44); putch(32);
}

fn hanoi(n:int, one:int, two:int, three:int)
{
    if (n == 1)
    {
        move(one, three);
    }
    else {
        hanoi(n - 1, one, three, two);
        move(one, three);
        hanoi(n - 1, two, one, three);
    }
}

fn main()->int
{   
    _sysy_starttime(21);
    let n:int = getint();
    while (n > 0) {
        hanoi(getint(), 1, 2, 3);
        putch(10);
        n = n - 1;
    }
    _sysy_stoptime(28);
    ret 0;
}
