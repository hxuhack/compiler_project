let N:int;

let newline:int;

fn factor(n:int)->int
{
    let i:int;
    let sum:int;
    sum=0;
    i=1;
    while(i<(n+1))
    {
        if(((n/i)*i) == n)
        {
            sum=sum+i;
        }
        i=i+1;
    }
    ret sum;
}

