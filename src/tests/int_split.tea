let N:int;

let newline:int;

fn mod(x:int,y:int)->int
{
    ret x-(x/y)*y;
}

fn split(n:int,a[4]:int)->int
{
    let i:int;
    i=N-1;
    while(i!=-1)
    {
        a[i]=mod(n,10);
        n=n/10;
        i=i-1;
    }

    ret 0;
}

